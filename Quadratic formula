#include <iostream>
#include <cmath>    // for sqrt
#include <string>
using namespace std;

string mainCalculation(double a, double b, double c) {
    double discriminant = (b * b) - (4 * a * c);

    if (discriminant > 0) {
        double root1 = (-b + sqrt(discriminant)) / (2 * a);
        double root2 = (-b - sqrt(discriminant)) / (2 * a);
        return "Your final answers are " + to_string(root1) + " and " + to_string(root2);
    } 
    else if (discriminant == 0) {
        double root = -b / (2 * a);
        return "Your final answer is " + to_string(root);
    } 
    else {
        return "No real roots.";
    }
}

int main() {
    double a, b, c;
    cout << "Give me a number for a: ";
    cin >> a;
    cout << "Give me a number for b: ";
    cin >> b;
    cout << "Give me a number for c: ";
    cin >> c;

    string result = mainCalculation(a, b, c);
    cout << result << endl;

    return 0;
}
